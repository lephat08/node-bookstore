<%- contentFor('title') %>
Delete
<%- contentFor('body') %>
<h1>Xóa người dùng</h1>

<form action="/delete/<%= model.id %>" method="post">
  <div class="form-horizontal">

    <%- include("_display") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Xóa" onclick="return confirm('Bạn có muốn xóa thật chứ ?!');" class="btn btn-default btn-danger" />
        <a class="btn btn-outline-dark cancel" href="/info">Hủy</a>
      </div>
    </div>
  </div>
</form>
