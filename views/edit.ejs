<%- contentFor('body') %>
<h1>Cập nhật thông tin nhân viên</h1>

<form action="/edit/<%= model.id %>" method="post">
  <div class="form-horizontal">

    <%- include("_editor") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Update" class="btn btn-default btn-warning" />
        <a class="btn btn-outline-dark cancel" href="/info">Cancel</a>
      </div>
    </div>
  </div>
</form>