<%- contentFor('title') %>
Create
<%- contentFor('body') %>
<h4 style="margin-bottom: 30px;">Thêm sách vào dữ liệu</h4>

<form action="/bookcreate" method="post" enctype='multipart/form-data' >
  <div class="form-horizontal">

    <%- include("_book_create") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Save" onclick="return confirm('Bạn có muốn cập nhật thật chứ ?!');" class="btn btn-default btn-success" />
        <a class="btn btn-outline-dark cancel" href="/book">Hủy</a>
      </div>
    </div>
  </div>
</form>
